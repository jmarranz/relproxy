<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>relproxy_test_itsnat</display-name>
    <servlet>
        <servlet-name>ItsNatGroovyServlet</servlet-name>
        <servlet-class>example.groovyex.ItsNatGroovyServlet</servlet-class>
        <init-param>
            <param-name>scriptRootPath</param-name>
            <param-value>groovyex/code</param-value>
        </init-param>
        <init-param>
            <param-name>initScript</param-name>
            <param-value>example/groovyex/groovy_servlet_init.groovy</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ItsNatGroovyServlet</servlet-name>
        <url-pattern>/groovyex</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ItsNatJProxyServlet</servlet-name>
        <servlet-class>example.javaex.JProxyExampleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ItsNatJProxyServlet</servlet-name>
        <url-pattern>/javaex</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
